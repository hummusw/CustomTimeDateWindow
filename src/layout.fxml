<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <bottom>
      <Button fx:id="button_settings" focusTraversable="false" mnemonicParsing="false" onAction="#action_settings" text="Settings" BorderPane.alignment="CENTER" />
   </bottom>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
   <center>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
          <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
              <children>
                  <Label fx:id="label_date" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Loading date..." textAlignment="CENTER" />
                  <Label fx:id="label_time" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Loading time..." textAlignment="CENTER">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
              </children>
          </VBox>
         </children>
      </HBox>
   </center>
</BorderPane>
